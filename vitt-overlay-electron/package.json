{
  "name": "vitt-overlay-ui",
  "version": "1.0.0",
  "description": "Run on macOS: ```bash npm install npm run start ``` Hotkeys: - Toggle click-through: **⌥ + `** - Hide/Show: **⌥ ⇧ H** - Reload: **⌥ ⇧ R**",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dev": "electron .",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@recallai/desktop-sdk": "^1.3.5",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "electron": "^38.1.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.VittAi.overlay",
    "productName": "Overlay-Vitt",
    "win": {
      "target": "portable",
      "icon": "build/vitt-logo.png"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.utilities",
      "icon": "build/icon.icns"
    },
    "directories": {
      "output": "dist-mac"
    }
  }
}
